{
    "name": "mannysoft/inv",
    "description": "Inventory management using Laravel 6",
    "keywords": ["laravel", "inventory", "inventory-management"],
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.3.0",
        "illuminate/database": "6.*",
        "illuminate/support": "6.*",
        "baum/baum": "1.0.* | 1.1.*"
    }
    "archive": {
      "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "Stevebauman\\Inventory\\": "src/"
        }
    },
    "autoload-dev":
    {
        "psr-4": {
          "Stevebauman\\Inventory\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
