{
    "name": "mannysoft/inv",
    "description": "Inventory management using Laravel 6",
    "keywords": ["laravel", "inventory", "inventory-management"],
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2.0",
        "illuminate/database": "^6.2",
        "illuminate/support": "^6.2"
    },
    "require-dev": {
        "orchestra/testbench": "3.1.*"
    },
    "archive": {
        "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "Stevebauman\\Inventory\\": "src/"
        }
    },
    "autoload-dev":
    {
        "psr-4": {
            "Stevebauman\\Inventory\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
